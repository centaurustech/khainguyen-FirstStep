@model IEnumerable<khainguyen_FirstStep.EntityBinhLuan>
<div class="space_20"></div>
<a href="#" class="btn btn-big show-popup-proj" data-id="popup1"><i class="fa-comment"></i> Gửi nhận xét</a>
@{
    foreach (var item in Model)
    {
        <div class="supporter-row">
            <div class="avatar"><img src="@item.EntityUser.Avatar" alt="" /></div>
            <a href="#"><h3>@item.EntityUser.HoTen</h3></a>
            <p>@item.NoiDung</p>
        </div>
    }
}

@model IEnumerable<khainguyen_FirstStep.EntityDauTu>

<div class="countSupport"><span class="tiny countTo" data-from="0" data-to="@ViewBag.tongso" data-speed="3000">@ViewBag.tongso</span> Người đã tham gia</div>
<div class="list-avatar">
    @{
    foreach (var item in Model)
    {
        string temp =  @item.EntityUser.HoTen + " đã tham gia " + 1 + " dự án";
        //string temp1 = "&lt;b class=&#39;tiny&#39;&gt;John Abc&lt;/b&gt;&lt;br /&gt;Support &lt;b&gt;1&lt;/b&gt; project";

        <a href="#" class="tooltip" title='@temp'><img src="@item.EntityUser.Avatar" /></a>
    }
}
</div>

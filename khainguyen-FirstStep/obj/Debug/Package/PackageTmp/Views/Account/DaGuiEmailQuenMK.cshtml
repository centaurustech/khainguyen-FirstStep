@{
    ViewBag.Title = "Lấy lại mật khẩu thành công";
}

@section MeTaHeader
{
    <meta name="robots" content="noindex,nofollow" />
}

<link href="~/Content/cssFrontEnd/css/page-account.css" rel="stylesheet" />
<div class="page">
    <div class="panel-list">
        <div class="background">
            <img src="~/Content/cssFrontEnd/upload/project/bg3-1366x768.jpg" alt="" />
        </div>
        <div class="container">

            <!-- acc-login -->
            <div class="panel-login">
                <h4>Lấy lại mật khẩu thành công</h4>
                <div class="form-login">
                   
                        <form>
                            <p>Mật khẩu mới đã được gửi qua email đăng ký của bạn. <br /> Đăng nhập email của bạn để kiểm tra.</p>
                            <p><a href="@Url.Action("Login", "Account")" class="btn btn-login-fb"><i class="fa-sign-in"></i>&nbsp;Quay lại đăng nhập</a></p>
                        </form>
                    
                </div>
            </div>

        </div>
    </div>
</div>

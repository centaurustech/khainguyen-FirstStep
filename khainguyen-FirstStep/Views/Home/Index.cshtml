    <script type="text/javascript">
        $(document).ready(function () {
            // staff pick
            var owl5 = $(".list-item-tag");

            owl5.owlCarousel({
                navigation: false,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true,
                pagination: false,
                touchDrag: false,
                mouseDrag: false
            });

            $('.list-tag a').click(function (e) {
                e.preventDefault();
                $('.list-tag a').removeClass('active');
                $(this).addClass('active');
                owl5.trigger('owl.goTo', $(this).index());
            });

        });
    </script>
